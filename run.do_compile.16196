}

bbfatal_log() {
	if [ -p /data2/sonlam/build/tmp/work/cortexa7hf-vfp-neon-poky-linux-gnueabi/u-boot/v2013.01.01+git9eefbd1f4e3160fca54d224447d75b93f30da6a5-r0/temp/fifo.16196 ] ; then
		printf "%b\0" "bbfatal_log $*" > /data2/sonlam/build/tmp/work/cortexa7hf-vfp-neon-poky-linux-gnueabi/u-boot/v2013.01.01+git9eefbd1f4e3160fca54d224447d75b93f30da6a5-r0/temp/fifo.16196
	else
		echo "ERROR: $*"
	fi
	exit 1

}

bbnote() {
