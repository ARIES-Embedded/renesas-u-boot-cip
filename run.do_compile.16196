do_compile() {
    if [ "" = "ld-is-gold" ] ; then
        sed -i 's/$(CROSS_COMPILE)ld$/$(CROSS_COMPILE)ld.bfd/g' config.mk
    fi

    unset LDFLAGS
    unset CFLAGS
    unset CPPFLAGS
    oe_runmake iwg23s_config
    oe_runmake all

}

oe_runmake() {
	oe_runmake_call "$@" || die "oe_runmake failed"

}

die() {
	bbfatal_log "$*"

}

oe_runmake_call() {
	bbnote make -j 2 CROSS_COMPILE=arm-poky-linux-gnueabi- CC="arm-poky-linux-gnueabi-gcc  --sysroot=/data2/sonlam/build/tmp/sysroots/iwg23s" "$@"
